{
	"compilerOptions": {
		"strict": true,
		"jsx": "preserve",
		"target": "esnext",
		"module": "esnext",
		"sourceMap": true,
		"skipLibCheck": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"lib": ["esnext", "dom"],
		"resolveJsonModule": true,
		"moduleResolution": "node",
		"useDefineForClassFields": true
	},
	"include": ["docs/.vuepress/config.ts"],
	"exclude": ["node_modules", "docs/.vuepress/dist"]
}
